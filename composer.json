{
  "name": "swoft/elasticsearch",
  "type": "library",
  "version": "v2.0.9",
  "keywords": [
    "php",
    "swoole",
    "swoft"
  ],
  "description": "micro-service framework base on swoole",
  "license": "Apache-2.0",
  "require": {
    "php": ">7.1",
    "swoft/stdlib": "~2.0.0",
    "swoft/connection-pool": "~2.0.0",
    "elasticsearch/elasticsearch": "^7.3",
    "composer/ca-bundle": "^1.2"
  },
  "autoload": {
    "psr-4": {
      "Swoft\\Elasticsearch\\": "src/"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^7.5"
  },
  "autoload-dev": {
    "psr-4": {
      "SwoftTest\\Elasticsearch\\Unit\\": "test/unit/",
      "SwoftTest\\Elasticsearch\\Testing\\": "test/testing/"
    }
  },
  "repositories": {
  },
  "scripts": {
    "test": "./vendor/bin/phpunit -c phpunit.xml"
  }
}
